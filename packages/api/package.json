{
  "name": "karmachain-api",
  "version": "0.5.1",
  "main": "./lib",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec 'yarn' build --watch src",
    "build": "babel src --out-dir lib",
    "browserify": "browserify --full-paths lib/index.js --standalone KarmaApi -o dist/karma-api.js -d",
    "postbrowserify": "uglifyjs --compress --mangle --sequences --drop_console --output dist/karma-api.min.js -- dist/karma-api.js"
  },
  "dependencies": {
    "karmajs-ws": "^0.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "browserify": "^16.2.3",
    "nodemon": "^1.18.9",
    "uglify-js-es6": "^2.8.9",
    "yargs": "^12.0.5"
  }
}
